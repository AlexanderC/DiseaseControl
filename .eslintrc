{
  "env": {
    "node": true,
    "es6": true
  },
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "rules": {
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-prototype-builtins": 0,
    "no-restricted-syntax": 0,
    "no-async-promise-executor": 0,
    "class-methods-use-this": 0,
    "no-unused-vars": 0,
    "no-empty-function": 0,
    "no-unsafe-negation": 0,
    "no-continue": 0,
    "no-await-in-loop": 0,
    "no-console": 0,
    "strict": 0
  }
}
